<%= simple_form_for(@applog) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :device %>
    <%= f.input :version %>
    <%= f.input :content %>
    <%= f.input :level %>
    <%= f.input :identifier %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
